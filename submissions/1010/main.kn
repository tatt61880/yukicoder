func main()
	var x: int :: cui@inputInt() * 1000
	var y: int :: cui@inputInt() * 1000
	var h: int :: cui@inputInt()
	if(x > y)
		do x :$ y
	end if
	var ans: int :: 0
	while(x > h)
		do h :* 4
		do y :* 2
		do ans :+ 1
	end while
	while(y > h)
		do h :* 4
		do x :* 2
		do ans :+ 1
	end while
	do cui@print("\{ans}\n")
end func