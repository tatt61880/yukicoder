func main()
	var n: int :: cui@inputInt()
	var z: int :: cui@inputInt()
	
	var ans: bool :: false
	
	if(n = 1)
		do ans :: z > 1
	elif(n = 2)
		for x(1, 1000)
			for y(1, 1000)
				if(x ^ 2 + y ^ 2 = z ^ 2)
					do ans :: true
				end if
			end for
		end for
	end if
	
	do cui@print(ans ?("Yes\n", "No\n"))
end func
