func main()
	var ans: bool :: true
	var s: []char :: cui@input()
	var j: int :: 0
	for i(0, ^s - 1)
		while(s[i] <> "kadomatsu!"[j])
			if("kadomatsu!"[j] = '!')
				do ans :: false
				break i
			end if
			do j :+ 1
		end while
		do j :+ 1
	end for
	do cui@print((ans ?("Yes", "No")) ~ "\n")
end func
