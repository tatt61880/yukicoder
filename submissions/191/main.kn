func main()
	var N: int :: cui@input().toInt(&)
	var ss: [][]char :: cui@input().split(" ")
	var C: []int :: #[N]int
	for i(0, N - 1)
		do C[i] :: ss[i].toInt(&)
	end for
	
	var sum: int
	for i(0, N - 1)
		do sum :+ C[i]
	end for
	
	var ans: int
	for i(0, N - 1)
		if(C[i] * 10 <= sum)
			do ans :+ 30
		end if
	end for
	do cui@print("\{ans}\n")
end func