func main()
	var ss: [][]char
	do ss :: cui@input().split(" ")
	var x: int :: ss[0].toInt(&)
	var y: int :: ss[1].toInt(&)
	do ss :: cui@input().split(" ")
	var x2: int :: ss[0].toInt(&)
	var y2: int :: ss[1].toInt(&)
	var ans: int :: [x, y].max()
	if(x = y & x2 = y2 & x2 < x)
		do ans :+ 1
	end if
	do cui@print("\{ans}\n")
end func