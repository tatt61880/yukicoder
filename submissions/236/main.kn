func main()
	var ss: [][]char :: cui@input().split(" ")
	var a: float :: ss[0].toFloat(&)
	var b: float :: ss[1].toFloat(&)
	var x: float :: ss[2].toFloat(&)
	var y: float :: ss[3].toFloat(&)
	if(a / b >= x / y)
		do y :: x * b / a
	else
		do x :: y * a / b
	end if
	var ans: float :: x + y
	do cui@print("\{ans.toStrFmt(".12f")}\n")
end func
