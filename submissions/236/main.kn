func main()
	var _: [][]char :: cui@input().split(" ")
	var A: float :: _[0].toFloat(&)
	var B: float :: _[1].toFloat(&)
	var X: float :: _[2].toFloat(&)
	var Y: float :: _[3].toFloat(&)
	if(A / B >= X / Y)
		do Y :: X * B / A
	else
		do X :: Y * A / B
	end if
	var ans: float :: X + Y
	do cui@print("\{ans.toStrFmt(".12f")}\n")
end func