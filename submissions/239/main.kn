func main()
	var n: int :: cui@inputInt()
	var a: []int :: #[n]int
	for i(0, n - 1)
		for j(0, n - 1)
			do a[j] :+ cui@inputStr() = "nyanpass" ?(1, 0)
		end for
	end for
	var ans: int :: -2
	for i(0, n - 1)
		if(a[i] = n - 1)
			do ans :: ans = -2 ?(i + 1, -1)
		end if
	end for
	do ans :: ans.clampMin(-1)
	do cui@print("\{ans}\n")
end func
