func main()
	var n: int :: cui@inputInt()
	var x: int :: cui@inputInt()
	var a: []int :: #[n]int
	var b: []int :: #[n]int
	for i(0, n - 1)
		do a[i] :: cui@inputInt()
		do b[i] :: cui@inputInt()
	end for
	
	for j(1, x)
		var ans: int :: 0
		for i(0, n - 1)
			do ans :: lib@max(ans, b[i] - (j - a[i]).abs())
		end for
		do cui@print("\{ans}" ~ (j = x ?("\n", " ")))
	end for
end func
