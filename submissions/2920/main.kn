func main()
	var s: []char :: cui@input()
	var t: []char :: cui@input()
	
	var a: int :: 0
	var b: int :: 0
	var ab: int :: 0
	var o: int :: 0
	for i(0, 1)
		for j(0, 1)
			var xx: []char :: [s[i], t[j]]
			do xx.sort()
			switch(xx)
			case "AA", "AO"
				do a :+ 25
			case "BB", "BO"
				do b :+ 25
			case "AB"
				do ab :+ 25
			default
				do o :+ 25
			end switch
		end for
	end for
	
	do cui@print("\{a} \{b} \{ab} \{o}\n")
end func
