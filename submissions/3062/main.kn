func main()
	var strs: [][]char :: cui@input().split(" ")
	var a: int :: strs[0].toInt(&)
	var b: int :: strs[1].toInt(&)
	var ans: int
	if(b < 0)
		do ans :: a + b
		do cui@print("\{ans}\n")
	elif(strs[1][0] = '+')
		do ans :: a + b
		do cui@print("\{ans}\n")
	elif(a = 0)
		do ans :: b
		do cui@print("\{ans}\n")
	else
		do cui@print("\{a}\{b}\n")
	end if
end func
