func main()
	var ss: [][]char
	do ss :: cui@input().split(" ")
	var a: int :: ss[0].toInt(&)
	var b: int :: ss[1].toInt(&)
	var ans: int
	if(b < 0)
		do ans :: a + b
		do cui@print("\{ans}\n")
	elif(ss[1][0] = '+')
		do ans :: a + b
		do cui@print("\{ans}\n")
	elif(a = 0)
		do ans :: b
		do cui@print("\{ans}\n")
	else
		do cui@print("\{a}\{b}\n")
	end if
end func
