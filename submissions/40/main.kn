func main()
	var d: int :: cui@inputInt()
	var a: []int :: #[d + 1]int
	for i(0, d)
		do a[i] :: cui@inputInt()
	end for
	for i(d, 3, -1)
		do a[i - 2] :+ a[i]
	end for
	
	do d :: 0
	for i(2, 0, -1)
		if(a[i] <> 0)
			do d :: i
			break i
		end if
	end for
	do cui@print("\{d}\n")
	for i(0, d)
		do cui@print("\{a[i]}")
		do cui@print(i = d ?("\n", " "))
	end for
end func
