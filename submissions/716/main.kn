func main()
	var ss: [][]char
	var n: int :: cui@input().toInt(&)
	do ss :: cui@input().split(" ")
	var a: []int :: #[n]int
	for i(0, n - 1)
		do a[i] :: ss[i].toInt(&)
	end for
	var min: int :: a[1] - a[0]
	for i(0, n - 2)
		do min :: [min, a[i + 1] - a[i]].min()
	end for
	var max: int :: a[n - 1] - a[0]
	do cui@print("\{min}\n")
	do cui@print("\{max}\n")
end func
