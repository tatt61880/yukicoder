func main()
	var N: int :: cui@input().toInt(&)
	var ss: [][]char :: cui@input().split(" ")
	var A: []int :: #[N]int
	var sum: int :: 0
	for i(0, N - 1)
		do A[i] :: ss[i].toInt(&)
		do sum :+ A[i]
	end for
	
	for i(0, N - 1)
		var ans: int :: (sum - A[i] * (N - 1))
		do cui@print("\{ans}\{i = N - 1 ?("\n", " ")}")
	end for
end func